{
    "name": "obviobysage/traits",
    "version": "1.0.0",
    "description": "A package of common traits for the Obv.io ecosystem",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ObvioBySage\\Traits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ObvioBySage\\Traits\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tyler Schwartz",
            "email": "tyler@obv.io"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "orchestra/testbench": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ObvioBySage\\Telemetry\\TelemtryServiceProvider"
            ]
        }
    }
}
